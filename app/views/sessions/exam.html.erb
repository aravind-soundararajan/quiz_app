<h1>Question Sets available</h1>
<%= form_tag( { :action => :ques, }, { :method => :post }) %>
<ul class="users">
<% @users.each do |user| %>
<%if user.admin? %>
<div class="clearfix">
<%= radio_button_tag(:email, user.email) %>&nbsp;<%= user.name %><br/></div>
<% else %>
<%end%>
<% end %>
<br/>
<br/>
<%= submit_tag("OK") %>
</ul>
